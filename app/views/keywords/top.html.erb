<div class="container">
  <%= render "share/error_message", error_message: @error_message %>
  <div class="show_wrapper">
    <%= form_with(model: @keywords, local: true, url: keyword_path, remote: true) do |form| %>
      <p><%= form.text_field :keyword, :placeholder => "秘密の合言葉を入力してください。", :size => "50×1", :class => "form" %></p>
      <%= form.submit value="決定", :class => "signup-btn" %>
    <% end %>
  </div>
</div>
